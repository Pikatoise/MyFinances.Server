{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "pgConn": "Server=postgres_db;Port=5432;Database=MyFinances;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Fixer": {
    "BaseAddress": "http://data.fixer.io/api/",
    "AccessKey": "",
    "Symbols": "USD,RUB",
    "Format": "1"
  }
}
